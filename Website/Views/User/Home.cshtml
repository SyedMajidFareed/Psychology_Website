@{
    ViewData["Title"] = "Home Page";
}
<div class = "container vh-100">
<div class="text-center">
    <h1 class="display-4">Welcome <b>@ViewBag.Name </b> </h1>
    
</div>
<div class = "table-responsive" >
<table class="table table-striped table-hover" style="background-color:#24a0ed; color:white;">
  <thead>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">Email</th>
      <th scope="col">Password</th>
      <th scope="col">Options</th>
      </tr>
  </thead>
  <tbody>

      @foreach(Website.Models.User u in Model)
        {
           <partial name="partialview" model="u"/>
        }
        
  </tbody>
</table>
</div>
</div>
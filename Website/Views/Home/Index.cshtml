@{
    ViewData["Title"] = "Home Page";
}

@model ContentData

<div class="text-center">
    <h1 class="display-4">Welcome
        <small class="text-muted">to Learn Psychology</small>
    </h1>

    @*Carousel Code*@
   
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="~/lib/img1.jpg" alt="...">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Human Cognition</h5>
                    <p>The mental action or process of acquiring knowledge and understanding through thought, experience, and the senses.</p>
                </div>
            </div>
            <div class="carousel-item">
                <img src="~/lib/img2.jfif" alt="...">
                <div class="carousel-caption d-none d-md-block">
                    <h5>img 2</h5>
                    <p>...</p>
                </div>
            </div>
            <div class="carousel-item">
                <img src="~/lib/img3.jpg" alt="...">
                <div class="carousel-caption d-none d-md-block">
                    <h5>img3</h5>
                    <p>...</p>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <h3 class="display-4">
        Categories
    </h3>
    <ul style="list-style:none">
        <li >
        <a href="#carouselExampleIndicators"><small class="text-muted">Modern Psychology</small></a>
            </li>
        <li style="text-decoration:none">
        <a href="#carouselExampleIndicators"><small class="text-muted">Quranic Psychology</small></a>
            </li>
        </ul>
    <h3 class="display-4">
        Contact a  <a asp-controller="User" asp-action="UserDetails"><small class="text-muted">Therapist</small></a>
    </h3>

    @*to add data*@

    @*<partial name="DataAddingPartialView"/>*@
   
</div>

@*<vc:content-data></vc:content-data>*@


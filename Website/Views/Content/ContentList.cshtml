@{
    ViewData["Title"] = "Content List";
}
@model List<ContentData>
<div class="container ">
    <div class="text-center">
        <h1 class="display-4">Welcome <b>@ViewBag.Name </b> </h1>

    </div>
    <div class="table-responsive">
        <table class="table table-striped table-hover" style="background-color:#24a0ed; color:white;">
            <thead>
                <tr>
                    <th scope="col">Topic</th>
                    <th scope="col">Category</th>
                    
                    <th scope="col">Options</th>
                </tr>
            </thead>
            <tbody>

                @foreach (var u in Model)
                {
                    <partial name="CTtablepartialView" model="u" />
                }

            </tbody>
        </table>
    </div>
</div>